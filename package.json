{
  "name": "@ap2/source",
  "version": "0.4.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .angular dist node_modules && npx nx clear-cache",
    "set-up-database": "npx prisma db push --force-reset --skip-generate && npx prisma generate --generator jsclient && npx prisma db seed",
    "set-up-database-ai": "npx prisma db push --force-reset --skip-generate && npx prisma generate --generator jsclient && npx prisma db seed ai",
    "sud": "npm run set-up-database",
    "sud-ai": "npm run set-up-database-ai",
    "clean-install": "npm run clean && npm install && npm run sud",
    "clean-install-ai": "npm run clean && npm install && npm run sud-ai",
    "dev": "npx nx run-many --all --parallel 3 --target serve",
    "dev-with-outlier": "concurrently \"npm run dev\" \"npm run start-outlier-detection\"",
    "start-outlier-detection": "./scripts/run-outlier-detection.sh",
    "test": "npx nx run-many --all --target test",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "fix-lint": "./scripts/run-eslint-fix.sh",
    "license-checker": "npm run license-checker-all && npm run license-checker-unknown && npm run license-checker-summary",
    "license-checker-all": "npx license-checker --unknown --csv --out=./third-party-licenses/third-party-licenses.csv",
    "license-checker-unknown": "npx license-checker --onlyunknown --csv --out=./third-party-licenses/third-party-licenses-unknown.csv",
    "license-checker-summary": "npx license-checker --unknown --summary --out=./third-party-licenses/third-party-licenses-summary.txt"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "private": true,
